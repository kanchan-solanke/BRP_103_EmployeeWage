<!DOCTYPE html>
<html>
<body>
<script>
       
       const IS_PART_TIME = 1;
        const IS_FULL_TIME = 2;    
        var wagePerHour = 20;
        var empHrs = 0;  
        var empWage = 0;
        var empCheck = Math.floor(Math.random() * 10) % 3;
        if (empCheck == IS_PART_TIME)
            empHrs = 4;
        else if (empCheck == IS_FULL_TIME)
            empHrs = 8;
        else
            empHrs = 0;
        empWage = empHrs * wagePerHour;
        console.log("Emp Wage:"+ empWage);
        
        </script>
        </body>
        </html>
